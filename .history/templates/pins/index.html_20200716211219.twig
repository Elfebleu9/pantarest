{% extends 'base.html.twig' %}

{% block title 'Find all the best pins on Earth !' %}

{% block body %}
    <h1>{{pluralize(pins|length,'Pin','Pins')}} pins</h1>

    {% for pin in pins %}
        <article>
            <h2>{{pin.title}}</h2><br/>
            <p>{{pin.description}}</p>
        </article>
    {% endfor %}


{% endblock %}
